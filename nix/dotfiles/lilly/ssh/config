Host *.eurofurence.org
  IdentityFile ~/.ssh/id_lilly@eurofurence.pub


# Global Override Settings
Host *
  ForwardAgent no
  AddKeysToAgent no
  Compression no
  ServerAliveInterval 0
  ServerAliveCountMax 3
  HashKnownHosts no
  UserKnownHostsFile ~/.ssh/known_hosts
  ControlMaster no
  ControlPath ~/.ssh/master-%r@%n:%p
  ControlPersist no
  SetEnv TERM=xterm-256color
