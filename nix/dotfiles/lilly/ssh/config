# Matchers which go first because they can retrigger config file parsing
Match tagged legacy
  KexAlgorithms +diffie-hellman-group1-sha1,diffie-hellman-group-exchange-sha1,diffie-hellman-group14-sha1
  Ciphers +aes128-cbc,aes192-cbc,aes256-cbc,3des-cbc
  HostkeyAlgorithms +ssh-rsa
  PubkeyAcceptedKeyTypes +ssh-rsa 

Match !localnetwork 2000::/3
  ProxyJump jumphost.z9.aut-num.de



Host rsync.net zh4525.rsync.net
  User zh4525
  HostName zh4525.rsync.net
  IdentityFile ~/.ssh/id_lilly@lly.sh.pub



